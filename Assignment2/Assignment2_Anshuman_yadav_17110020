        "MLE = []\n",
        "for i in range(4):\n",
        "  d = dict()\n",
        "  if(i == 0):\n",
        "    for j in igram[i][0]:\n",
        "      d[j] = igram[i][0][j]/igram[i][1]\n",
        "    MLE.append(d)\n",
        "  else:\n",
        "    for j in igram[i][0]:\n",
        "      d[j] = igram[i][0][j]/igram[i-1][0][j[:-1]]\n",
        "    MLE.append(d)"
