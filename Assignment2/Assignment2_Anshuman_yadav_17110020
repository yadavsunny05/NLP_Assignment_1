        "f = open('speeches.txt', 'r', encoding='UTF-8')\n",
        "raw = f.read()\n",
        "raw = raw.replace(\"\\'\", \"\")\n",
        "raw = raw.replace(\",\", \"\")\n",
        "raw = raw.replace(\"\\r\\n\", \"\")\n",
        "raw = raw.replace(\"\\ufeff\",\"\")\n",
        "raw = re.sub(r'SPEECH \\d',r'',raw)\n",
        "nltk.download('punkt')"
