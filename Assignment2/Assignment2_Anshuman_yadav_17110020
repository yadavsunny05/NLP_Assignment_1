        "def Generator(n,size, igram):\n",
        "  ans = \"<s>\"\n",
        "  sentence = [\"<s>\"]\n",
        "  if(n == 1):\n",
        "    keys = list(MLE[n].keys())\n",
        "    values = list(MLE[n].values())\n",
        "    rand = np.random.multinomial(1 , values , size=size)\n",
        "    for i in rand:\n",
        "      for j in range(len(keys)):\n",
        "        if(i[j] == 1):\n",
