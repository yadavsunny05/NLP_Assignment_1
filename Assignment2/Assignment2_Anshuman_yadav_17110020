      "source": [
        "special_chars = re.compile('[`~!@#$%^&*()+={}|\\[\\]:\";<>?,\\./“”]')\n",
        "raw_sentence = sent_tokenize(raw)\n",
        "sentences = []\n",
        "for item in raw_sentence:\n",
        "  text = item.replace(\"\\n\", \" \")\n",
        "  text = \"<s> \" + special_chars.sub(\"\", text) + \" </s>\"\n",
        "  text = text.lower()\n",
        "  sentences.append(' '.join(text.split()))\n",
        "  \n",
        "print(sentences)"
      ],
