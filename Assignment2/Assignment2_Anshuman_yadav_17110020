        "def retngrams(xtrain,n):\n",
        "  dictk=dict()\n",
        "  count = 0\n",
        "  for sen in xtrain:\n",
        "    kgrams = ngrams(sen.split(), n)\n",
        "    for i in kgrams:\n",
        "      if(i not in dictk):\n",
        "        dictk[i]=1\n",
        "      else:\n",
        "        dictk[i]=dictk[i]+1\n",
        "      count=count+1\n",
        "  return(dictk,count)\n",
        "  "
      ],
      "execution_count": 0,
      "outputs": []
    },
