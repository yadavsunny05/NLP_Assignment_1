      "source": [
        "def Prob(sentence, n):\n",
        "  prob = 0\n",
        "  sentences = sentence.split(\" \")\n",
        "  if(n==1):\n",
        "    flag = 1\n",
        "    sentence = sentence[1:-1]\n",
        "  elif(n==2):\n",
        "    flag = 2\n",
        "  elif(n==3):\n",
        "    flag = 3\n",
        "  else:\n",
        "    flag = 4\n",
        "  for i in range(len(sentences)-n+1):\n",
        "    w = (sentences[i:i+n])\n",
        "    prob += math.log(MLE[flag -1][w],2)\n",
        "  return math.pow(2,prob)\n",
        "\n",
        "# function to calculate perplexity of all sentences given n_gram model\n",
        "def Perplexity(sentences, n):\n",
        "  tot_len = 0\n",
        "  prob = 0\n",
        "  for i in sentences:\n",
        "    prob -= math.log(Prob(i, n),2)\n",
        "    prob -= 0\n",
        "    sentence = i.split(\" \")\n",
        "    tot_len += len(sentence)-n-1\n",
        "  return math.pow(2, prob/tot_len)\n",
        "\n",
        "print(\"unigram perplexity: \",Perplexity(xtest, 1))\n",
        "print(\"bigram perplexity: \",Perplexity(xtest, 2))\n",
        "print(\"trigram perplexity: \",Perplexity(xtest, 3))\n",
        "print(\"quadgram perplexity: \",Perplexity(xtest, 4))"
      ],
      "execution_count": 143,
